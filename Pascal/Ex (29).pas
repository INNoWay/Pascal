Program Mes ;
var mes:integer;
 Begin
 writeln('entre com numero do mes');
 read(mes);
 case mes of
 1:writeln('janeiro');
 2:writeln('feverero');
 3:writeln('março');
 4:writeln('abril');
 5:writeln('maio');
 6:writeln('junho');                
 7:writeln('julho');
 8:writeln('agosto');
 9:writeln('setembro');
 10:writeln('outubro');
 11:writeln('novenbro');
 12:writeln('desenbro');
 else
 writeln('mes invalido');
 end;
 
 
  
 End.
